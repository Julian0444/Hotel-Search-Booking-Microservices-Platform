  token: {{admin_jwt_token}}
}

docs {
  Elimina un hotel por su ID. Requiere autenticación JWT de administrador.
  Cambia el hotel_id en la URL según necesites.
}

params:path {
  hotel_id: hotel123
}

settings {
  encodeUrl: true
}
