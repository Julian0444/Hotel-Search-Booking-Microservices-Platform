meta {
  name: Get Reservations by User ID
  type: http
  seq: 8
}

get {
  url: http://localhost:8081/users/{{user_id}}/reservations
  body: none
  auth: bearer
}

auth:bearer {
  token: {{jwt_token}}
}

docs {
  Obtiene todas las reservas de un usuario específico.
  Requiere autenticación JWT.
  Solo puedes ver tus propias reservas (a menos que seas administrador).
  Cambia el user_id en la URL según necesites.
}

params:path {
  user_id: 1
}

settings {
  encodeUrl: true
}
